GPP_CMD="arm-none-linux-gnueabi-g++ -g -O0 -fpermissive -rdynamic"
LINK_LIB="-lpthread -static-libstdc++ -static-libgcc"

TRG=mainvpd

$GPP_CMD ${TRG}.cpp i2c.c  ../miner_gate/squid.cpp ../miner_gate/spond_debug.cpp -o ${TRG} ${LINK_LIB} -I../miner_gate

chmod +x ${TRG}
cp ./${TRG} ~/shared/ 
cp ./${TRG} ~/shared/scripts

