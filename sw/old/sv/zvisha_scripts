# SINGLE CPU
_ REG 00FFFF40
_ REG 00FFFF01 0
_ REG 00FFFF40
_ REG 00FFFF00 0
_ REG 00FFFF40
_ REG 00FFFF00 0
_ REG 00FFFF40
# 1 chip on system
_ REG 00FFFF15 0


# 2 CPUS
_ REG 00FFFF40
_ REG 00FFFF01 0
_ REG 00FFFF40
_ REG 00FFFF00 0
_ REG 00FFFF40
_ REG 00FFFF00 1
_ REG 00FFFF40
# 2 chip on system
_ REG 00FFFF15 1



# 8000 CPUS
_ REG 00FFFF40
_ REG 00FFFF01 0
_ REG 00FFFF40
_ REG 00FFFF00 0
_ REG 00FFFF40
_ REG 00FFFF00 1
_ REG 00FFFF40
# 2 chip on system
_ REG 00FFFF15 16







